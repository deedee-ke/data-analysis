   "source": [
    "# Calculate success metrics for each author\n",
    "success_metrics <- amazon_books %>%\n",
    "  group_by(Author) %>%\n",
    "  summarise(\n",
    "    total_appearances = n(),\n",
    "    average_rating = mean(User.Rating),\n",
    "    total_reviews = sum(Reviews)\n",
    "  ) %>%\n",
    "  arrange(desc(total_reviews))\n",
    "head(success_metrics)"
   ]
  },
